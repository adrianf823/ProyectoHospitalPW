{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "ComposeMongo",
    "public": true
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Hospitales": {
    "dataSource": "ComposeMongo",
    "public": true
  },
  "Medicos": {
    "dataSource": "ComposeMongo",
    "public": true
  },
  "Usuario": {
    "dataSource": "ComposeMongo",
    "public": true
  },
  "userCredential": {
    "dataSource": "ComposeMongo",
    "public": false
  },
  "userIdentity": {
    "dataSource": "ComposeMongo",
    "public": false
  },
  "container": {
    "dataSource": "files-local",
    "public": true
  }

}
